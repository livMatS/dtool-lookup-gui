<glade-catalog name="dtool_gtk_catalog" 
               library="dtool_gtk"
               domain="glade-3" 
               depends="gtk+" 
               version="0.1"
               supports="gtkbuilder"
               language="python">
  <glade-widget-classes>
    <glade-widget-class title="dtool dataset list box" name="DtoolDatasetListBox" 
                        generic-name="DtoolDatasetListBox" parent="GtkListBox"
                        icon-name="widget-gtk-listbox"/>
    <glade-widget-class title="dtool dataset list box row" name="DtoolDatasetListBoxRow" 
                        generic-name="DtoolDatasetListBoxRow" parent="GtkListBoxRow"
                        icon-name="widget-gtk-listboxrow"/>
    <glade-widget-class title="dtool graph widget" name="DtoolGraphWidget"
                        generic-name="DtoolGraphWidget" parent="GtkDrawingArea"
                        icon-name="widget-gtk-drawing-area"/>
  </glade-widget-classes>
</glade-catalog>
